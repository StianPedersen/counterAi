{
    "data": {
        "images_path": "data_xml/images",
        "labels_path": "data_xml/labels",
        "source_label_format": "xml",
        "class_mapping": {
            "0": "lbattery",
            "1": "cylinder",
            "2": "mbattery"
        },
        "target_classes": [
            "lbattery",
            "cylinder",
            "mbattery"
        ],
        "train_split": 0.8
    },
    "detection": {
        "confidence_threshold": 0.9,
        "nms_threshold": 0.7,
        "model_config": "COCO-Detection/faster_rcnn_R_50_FPN_3x.yaml",
        "num_classes": 3,
        "max_iter": 3000,
        "batch_size": 2,
        "learning_rate": 0.00025
    },
    "tracking": {
        "max_disappeared": 30,
        "max_distance": 50,
        "tracker_type": "deepsort"
    },
    "counting_line": {
        "type": "horizontal",
        "position": "middle",
        "coordinates": [
            0,
            240,
            640,
            240
        ],
        "direction": "bottom_to_top"
    },
    "counting": {
        "enable_size_filter": false,
        "min_object_size": 0,
        "enable_confidence_filter": true,
        "min_confidence": 0.6
    },
    "output": {
        "save_video": true,
        "save_csv": true,
        "show_realtime": true,
        "output_dir": "output",
        "log_level": "INFO"
    },
    "performance": {
        "target_fps": 1,
        "device": "cuda"
    }
} 